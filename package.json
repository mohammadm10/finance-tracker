{
  "name": "express-server-example",
  "version": "1.0.0",
  "description": "A simple ExpressJS server in TypeScript with nodemon",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "nodemon --watch src -e ts --exec \"ts-node\" src/index.ts",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.0",
    "jwks-rsa": "^3.0.1",
    "mysql2": "^3.6.2",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "nodemon": "^2.0.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.0"
  }
}
